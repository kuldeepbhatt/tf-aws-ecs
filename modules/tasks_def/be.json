[{
        "name": "mongo",
        "family": "mongo",
        "image" : "${mongo_imagename}",
        "cpu": 10,
        "memory": 128,
        "essential": true,
        "hostname": "${hostname}",
        "portMappings": [{
            "containerPort": 27017,
            "hostPort": 27017
        }]
    },
    {
        "name": "${be_image_name}:${version}",
        "image": "${be_image_url}",
        "cpu": 10,
        "memory": 512,
        "essential": false,
        "links": "${links}",
        "portMappings": [{
            "containerPort": 80,
            "hostPort": 9000
        }]
        
    }
]